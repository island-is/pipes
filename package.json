  "workspaces": [
    "apps/*",
    "ci",
    "base/*",
    "pipes-modules/*"
  ],
  "devDependencies": {
    "@commitlint/config-conventional": "17.7.0",
    "@island-is/eslint-config": "workspace:*",
    "@island-is/prettier-config": "workspace:*",
    "@island-is/scripts": "workspace:*",
    "@island-is/tsconfig": "workspace:*",
    "@swc/core": "1.3.78",
    "@types/semver": "7.5.3",
    "@yarnpkg/types": "4.0.0-rc.52",
    "commitlint": "17.7.1",
    "eslint": "8.50.0",
    "express": "4.18.2",
    "glob": "10.3.10",
    "husky": "8.0.3",
    "prettier": "3.0.3",
    "semver": "7.5.4",
    "typescript": "5.1.6",
    "zod": "3.22.2"
  },
  "engines": {
    "node": "20.5.1",
    "yarn": "4.0.0-rc.47",
    "npm": "please use yarn",
    "pnpm": "please use yarn"
  },
  "peerDependencies": {
    "@swc/core": "*"
  }
